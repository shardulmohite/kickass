
<label class="label"><%= t("activerecord.attributes.ass.name", :default => t("activerecord.labels.name", :default => "Name")) %>:</label>
<p><%= @ass.name %></p>

<div class="form-actions">
<%= link_to "Back", asses_path, :class => 'btn'  %>
<%= link_to "Edit", edit_ass_path(@ass), :class => 'btn' %>
<%= link_to "Delete", ass_path(@ass), :method => "delete", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}", :class => 'btn' %>
</div>
